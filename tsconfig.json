{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "jsx": "react-native",
    "lib": ["dom", "esnext"],
    "moduleResolution": "node",
    "noEmit": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "strict": true,
    "allowJs": true,
    "baseUrl": "./",
    "paths": {
      "components/*": [
        "./src/components/*"
      ],
      "res/*": [
        "./src/res/*"
      ],
      "library/*": [
        "./src/library/*"
      ],
    }
  },
}
